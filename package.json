{
  "name": "minecraft-events",
  "version": "0.0.1",
  "description": "Uses RCON polling to get online stats.",
  "main": "build/",
  "scripts": {
    "start": "node build",
    "build": "babel --presets es2015 source -d build",
    "watch": "nodemon --watch source --exec \"npm run build && npm run start\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dodekeract/minecraft-events.git"
  },
  "keywords": [
    "minecraft",
    "rcon",
    "events",
    "polling",
    "online",
    "join",
    "leave"
  ],
  "author": "Florian Wendelborn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dodekeract/minecraft-events/issues"
  },
  "homepage": "https://github.com/dodekeract/minecraft-events#readme",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0"
  },
  "dependencies": {
    "rcon": "CodeKingdomsTeam/node-rcon"
  }
}
